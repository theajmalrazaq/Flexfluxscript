{
  "manifest_version": 3,
  "name": "FlexFlux Custom Styler",
  "version": "1.2",
  "description": "A Chrome extension to apply custom styles on FlexStudent pages.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://flexstudent.nu.edu.pk/*"],
      "js": ["homeScript.js"]
    }
  ],
  "host_permissions": ["https://flexstudent.nu.edu.pk/*"]
}
